{
  "name": "weatherbitclient",
  "version": "1.0.0",
  "description": "",
  "main": "src/app.ts",
  "scripts": {
    "start:dev": "nodemon",
    "swagger": "tsoa spec",
    "build": "tsc ",
    "start": "npm run build && node build/app.js",
    "predev": "npm run swagger",
    "prebuild": "npm run swagger",
    "dev": "concurrently \"nodemon\" \"nodemon -x tsoa spec\"",
    "test": "mocha -r ts-node/register test/**/*.test.ts --exit ",
    "test:coverage": "nyc npm run test --exit ",
    "eslint": "npx eslint src/*/*.** ",
    "eslint:fix": "npx eslint --fix src/*/*.** "
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "check-coverage": true,
    "all": true,
    "include": [
      "src/**/!(*.test.*).[tj]s?(x)"
    ],
    "exclude": [
      "src/_tests_/**/*.*"
    ],
    "reporter": [
      "html",
      "lcov",
      "text",
      "text-summary"
    ],
    "report-dir": "coverage"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.1.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "joi": "^17.6.3",
    "node-schedule": "^2.1.0",
    "redis-om": "^0.3.6",
    "swagger": "^0.7.5",
    "swagger-ui": "^4.14.3",
    "swagger-ui-express": "^4.5.0",
    "ts-node": "^10.9.1",
    "tsoa": "^4.1.3",
    "typescript": "^4.8.4"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/axios": "^0.14.0",
    "@types/chai": "^4.3.3",
    "@types/chai-http": "^4.2.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.14",
    "@types/joi": "^17.2.3",
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.11.0",
    "@types/node-schedule": "^2.1.0",
    "@types/nodemon": "^1.19.2",
    "@types/swagger-ui": "^3.52.0",
    "@types/swagger-ui-express": "^4.1.3",
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "concurrently": "^7.4.0",
    "mocha": "^10.1.0",
    "nodemon": "^2.0.20",
    "nyc": "^15.1.0"
  }
}